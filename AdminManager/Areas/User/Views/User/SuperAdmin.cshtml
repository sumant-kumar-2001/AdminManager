@model IEnumerable<DealerVM>
@{
    ViewData["Title"] = "SuperAdmin";
}


<div class=" row pt-4">
    <div class="col-6">
@*    <h1>SuperAdmin</h1>
*@    </div>
    <div class=" col-6 text-end">
        <div>
            <a asp-area="User" asp-controller="User" asp-action="AddAdmin" class="btn btn-primary">
                <i class="bi bi-person-plus-fill"></i> &nbsp; Add Admin
            </a>
            <a asp-area="User" asp-controller="User" asp-action="Admin" class="btn btn-primary">
                <i class="bi bi-person-plus-fill"></i> &nbsp; Dealer List
                </a>
        
            <a asp-area="User" asp-controller="User" asp-action="AllProduct" class="btn btn-primary">
                <i class="bi bi-person-plus-fill"></i> &nbsp; Product List
            </a>
            <a asp-area="User" asp-controller="User" asp-action="AddRole" class="btn btn-primary">
                <i class="bi bi-person-plus-fill"></i> &nbsp; Add New Role
            </a>
        </div>
   </div>
    </div>

<div class="container p-3">
    <div class=" row pt-4">   <div class="col-6">
            <h2 class="text-primary">Admin List </h2>
        
    </div>
 </div>

 


    <br /><br />


    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                   Admin Name
                </th>
                <th>
                    Email
                </th>
                <th>
                    PhoneNumber
                </th>
               
            </tr>

        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="30%">
                        @obj.UserName
                    </td>
                    <td width="30%">
                        @obj.Email
                    </td>
                    <td>
                        @obj.PhoneNumber
                    </td>
                  
                </tr>
            }
        </tbody>
    </table>
</div>


